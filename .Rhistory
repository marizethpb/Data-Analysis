#Performing Muliple Regression and Checking for the assumptions 

# Reading the files and importing the necessary libraries
setwd("D:/OneDrive - Polytechnic University of the Philippines/R")
consumptionthesis <- read.csv("19.csv", header=TRUE, sep = ",")
library(lmtest)
library(car)

regmodel <- lm(log_RHC ~ Real_HP + R_equity +  R_Budget, data=consumptionthesis )

summary(regmodel)
dwtest(regmodel)
shapiro.test(rstandard(regmodel))
ncvTest(regmodel)
vif(regmodel)
bptest(regmodel)
reset(regmodel)
plot(regmodel)
